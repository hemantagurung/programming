

public class MyClass3
{
   public static void main(String[] args){
    
   }
   public void printeach(int[] arr){
       for(int i=0; i< arr.length; i++){
           System.out.println(arr[i]);
       }
   }
    }


public class AreaOfRectangle
{
   public static void main(String[] args)
   {
       int a=2;
       int b=3;
       int c=a*b;
       
       System.out.println("area of rectangle=" +c);
       
   }
}


public class SquareOfTheNumber
{
   public static void main(String []args)
   {
       double a=3.9; // double because it is decimal number
       double b=a*a;
       
       System.out.println("the square number of a is "+b);
    }
} 


public class FarenheitToCelcius
{
    public static void main(String [] args)
    {
        float Farenheit,Celcius;
        Farenheit = 99;
        Celcius = ((Farenheit-32)*5)/9;
        System.out.println("Temperature in Celcius is;" +Celcius);
    }
}
