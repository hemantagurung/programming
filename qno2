

public class MyClass{

   public static void main(String[] args){
    
   }
   public void printeach(int[] arr){
       for(int i=0; i< arr.length; i++){
           System.out.println(arr[i]);
       }
   }
    }


public class AreaOfRectangle
{
   public static void main(String[] args)
   {
       int a=2;
       int b=3;
       int c=a*b;
       
       System.out.println("area of rectangle=" +c);
       
   }
}


public class SquareOfTheNumber
{
   public static void main(String []args)
   {
       double a=3.9; // double because it is decimal number
       double b=a*a;
       
       System.out.println("the square number of a is "+b);
    }
} 


public class FarenheitToCelcius
{
    public static void main(String [] args)
    {
        float Farenheit,Celcius;
        Farenheit = 99;
        Celcius = ((Farenheit-32)*5)/9;
        System.out.println("Temperature in Celcius is;" +Celcius);
    }
}


public class AreaAndPerimeterOfCircle
{
   public static void main(String []args)
   {
       int radius;
       double pi = 22/7 , area, perimeter;
       radius=7; 
        
       area = pi * radius * radius;  //calculating the area of the circle
       System.out.println("Area of the Circle is :" +area);
       
       perimeter = 2 * pi * radius;  //calcutaing the perimeter of circle
       System.out.println("perimeter of the circle is :" +perimeter);
       
   }
    }


public class SwappingTwoVariables
{
    public static void main(String[] args)
    {
        int a=6;
        int b=7;
        
        int temp=a;
        a=b;
        b=temp;
        
        System.out.println("the value of a is"+a);
        System.out.println("the value of b is"+b);
    }
     }
